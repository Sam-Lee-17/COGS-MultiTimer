{
  "name": "Multi-Timer",
  "description": "Use multiple timers in your project!",
  "version": "0.2.0",
  "icon": "chess-clock",
  "events": {
    "fromCogs": [
      {
        "name": "Create Timer",
        "value": { "type": "string" }
      },
      {
        "name": "Delete Timer",
        "value": { "type": "string" }
      },
      {
        "name": "Start Timer",
        "value": { "type": "string" }
      },
      {
        "name": "Stop Timer",
        "value": { "type": "string" }
      },
      {
        "name": "Reset Timer",
        "value": { "type": "string" }
      },
      {
        "name": "Set Time",
        "value": { "type": "number", "default": 30, "min": 0 }
      },
      {
        "name": "Set Default Time",
        "value": { "type": "number", "default": 30, "min": 0 }
      }
    ],
    "toCogs": [
      {
        "name": "Timer Started",
        "value": { "type": "string" }
      },
      {
        "name": "Timer Stopped",
        "value": { "type": "string" }
      },
      {
        "name": "Timer Finished",
        "value": { "type": "string" }
      },
      {
        "name": "Timer Reset",
        "value": { "type": "string" }
      }
    ]
  },
  "minCogsVersion": "4.11.0",
  "state": [
    {
      "name": "Select Timer",
      "value": {
        "type": "string",
        "default": ""
      },
      "writableFromCogs": true
    },
    {
      "name": "Selected Timer",
      "value": {
        "type": "string",
        "default": "None"
      },
      "writableFromClient": true
    },
    {
      "name": "Selected Timer Time",
      "value": {
        "type": "number",
        "default": 0
      },
      "writableFromClient": true
    },
    {
      "name": "Selected Timer Default Time",
      "value": {
        "type": "number",
        "default": 0
      },
      "writableFromClient": true
    },
    {
      "name": "Selected Timer Active",
      "value": {
        "type": "boolean",
        "default": false
      },
      "writableFromClient": true
    }
  ]
}
